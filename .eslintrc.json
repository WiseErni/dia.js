{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": "2020"
    },
    "extends": [
        "eslint:recommended", "plugin:redos/recommended"
    ],
    "globals": {
        "ZERO_UUID": "readonly",
        "darn": "readonly",
        "clone": "readonly",
        "suicide": "readonly",
        "dt_iso": "readonly"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-fallthrough": "off",
        "no-case-declarations": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-useless-escape": "off"
    }
}
